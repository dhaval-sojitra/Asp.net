@model List<Crud.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <thead>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Delete</th>
        <th>Edit</th>
    </thead>
@foreach (var item in Model)
{
<tr>
    <td>@item.Id</td>
    <td>@item.fname</td>
    <td>@item.lname</td>
    <td>@Html.ActionLink("Delete", "Delete" , new { id=item.Id })</td>
    <td>@Html.ActionLink("Edit","Registration",new { id = item.Id })</td>
</tr>

}
</table>

